{
  "name": "simple-react",
  "scripts": {
    "prestarter-global": "echo Installing required global packages...",
    "starter-global": "npm install gulp-cli@latest jspm@0.17.0-beta.47 -g",
    "starter-all": "npm run starter-global && npm run starter",
    "prestarter": "echo Preparing project...",
    "starter": "npm install && jspm install"
  },
  "dependencies": {
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  },
  "devDependencies": {
    "@types/material-ui": "^0.18.5",
    "@types/react": "^16.4.1",
    "@types/react-dom": "^16.0.6",
    "@types/react-router-dom": "^4.2.3",
    "awesome-typescript-loader": "^3.5.0",
    "jspm": "^0.17.0-beta.48",
    "pushstate-server": "^3.0.1",
    "simplr-gulp": "^2.8.3",
    "simplr-tslint": "0.0.1",
    "source-map-loader": "^0.2.3",
    "tslint": "^5.10.0",
    "typescript": "^2.9.2"
  },
  "jspm": {
    "name": "simple-react",
    "main": "app/index.js",
    "directories": {
      "packages": "wwwroot/libs",
      "baseURL": "wwwroot"
    },
    "configFiles": {
      "jspm": "src/configs/jspm.config.js"
    },
    "dependencies": {
      "history": "npm:history@^4.7.2",
      "material-ui": "npm:material-ui@^0.20.0",
      "react-router": "npm:react-router@^4.2.0",
      "react-router-dom": "npm:react-router-dom@^4.2.2",
      "tslib": "npm:tslib@^1.8.0"
    },
    "devDependencies": {},
    "peerDependencies": {
      "assert": "npm:jspm-nodelibs-assert@^0.2.0",
      "buffer": "npm:jspm-nodelibs-buffer@^0.2.0",
      "child_process": "npm:jspm-nodelibs-child_process@^0.2.0",
      "constants": "npm:jspm-nodelibs-constants@^0.2.0",
      "crypto": "npm:jspm-nodelibs-crypto@^0.2.0",
      "domain": "npm:jspm-nodelibs-domain@^0.2.0",
      "events": "npm:jspm-nodelibs-events@^0.2.0",
      "fs": "npm:jspm-nodelibs-fs@^0.2.0",
      "http": "npm:jspm-nodelibs-http@^0.2.0",
      "https": "npm:jspm-nodelibs-https@^0.2.0",
      "os": "npm:jspm-nodelibs-os@^0.2.0",
      "path": "npm:jspm-nodelibs-path@^0.2.0",
      "process": "npm:jspm-nodelibs-process@^0.2.0",
      "react": "npm:react@^16.0.0-beta.5",
      "react-dom": "npm:react-dom@^16.0.0",
      "stream": "npm:jspm-nodelibs-stream@^0.2.0",
      "string_decoder": "npm:jspm-nodelibs-string_decoder@^0.2.0",
      "url": "npm:jspm-nodelibs-url@^0.2.0",
      "util": "npm:jspm-nodelibs-util@^0.2.0",
      "vm": "npm:jspm-nodelibs-vm@^0.2.0",
      "zlib": "npm:jspm-nodelibs-zlib@^0.2.0"
    },
    "overrides": {
      "npm:browserify-zlib@0.1.4": {
        "dependencies": {
          "readable-stream": "^2.0.2",
          "pako": "~0.2.0"
        },
        "map": {
          "_stream_transform": "readable-stream/transform"
        }
      },
      "npm:inherits@2.0.3": {
        "ignore": [
          "test.js"
        ]
      },
      "npm:pbkdf2@3.0.14": {
        "main": "browser.js"
      },
      "npm:safe-buffer@5.1.1": {
        "browser": "index.js"
      },
      "npm:tslib@1.8.0": {
        "format": "cjs",
        "meta": {
          "tslib.js": {
            "esModule": true
          }
        }
      }
    }
  }
}
